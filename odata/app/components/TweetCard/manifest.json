{
	"_version": "1.14.0",
	"sap.app": {
		"id": "utg.components.TweetCard",
		"type": "card",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "utg.components.TweetCard.view.Main",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.38",
			"libs": {
				"sap.m": {}
			}
		}
	},
	"sap.card": {
		"type": "Component",
		"header": {
			"title": "{userName}",
			"subTitle": "@{screenName}",
			"icon": {
				"src": "{userPicUrl}"
			}
		}
	}
}
