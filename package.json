{
  "name": "sapui5-fiori3",
  "version": "0.2.0",
  "description": "Sample of an OpenUI5 app",
  "private": true,
  "scripts": {
    "start": "cds run",
    "start-ui5-app": "ui5 serve --h2 -p 5566",
    "start-odata-app": "cd odata && cds run",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "rimraf coverage && karma start karma-ci.conf.js",
    "watch": "npm run karma",
    "test": "npm run lint && npm run karma-ci",
    "build": "rimraf dist && ui5 build -a",
    "build-self-contained": "rimraf dist && ui5 build self-contained -a",
    "serve-dist": "ws --compress -d dist"
  },
  "dependencies": {
    "@sap/cds": "^3.13.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "@ui5/cli": "^1.7.0",
    "eslint": "^5.16.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-ui5": "^1.0.0",
    "local-web-server": "^3.0.4",
    "rimraf": "^2.6.2"
  }
}
