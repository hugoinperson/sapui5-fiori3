{
	"_version": "1.12.0",
	"sap.app": {
		"id": "utg.pages.Tweets",
		"type": "component",
		"embeds": [],
		"embeddedBy": "../App"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.tnt": {}
			}
		},
		"rootView": {
			"viewName": "utg.pages.Tweets.view.Index",
			"type": "XML",
			"async": true,
			"id": "tweets"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "utg.pages.Tweets.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"async": true,
				"viewType": "XML",
				"viewPath": "utg.pages.Tweets.view",
				"controlId": "indexTweets",
				"transition": "show",
				"controlAggregation": "pages"
			},
			"routes": {
				"main": {
					"pattern": "",
					"target": "main"
				}
			},
			"targets": {
				"main": {
					"viewName": "Main"
				}
			}
		}
	}
}
