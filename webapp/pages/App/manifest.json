{
	"_version": "1.12.0",
	"sap.app": {
		"id": "utg.pages.App",
		"type": "application",
		"embeds": ["utg/TwitterPage", "utg/LinkedInPage", "utg/FacebookPage"],
		"embeddedBy": "./"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.tnt": {}
			}
		},
		"componentUsages": {
			"twitterPage": {
				"name": "utg.pages.Twitter",
				"settings": {},
				"componentData": {},
				"lazy": false
			}
		},
		"rootView": {
			"viewName": "utg.pages.App.view.Index",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "utg.pages.App.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"async": true,
				"viewType": "XML",
				"viewPath": "utg.pages.App.view",
				"controlId": "indexApp",
				"transition": "show",
				"controlAggregation": "pages"
			},
			"routes": {
				"main": {
					"pattern": "",
					"target": "main"
				},
				"twitter": {
					"pattern": "twitter",
					"target": {
						"name": "twitter",
						"prefix": "atch"
					}
				}
			},
			"targets": {
				"main": {
					"viewName": "Main"
				},
				"twitter": {
					"type": "Component",
					"usage": "twitterPage",
					"options": {
						"manifestFirst": true,
						"async": true
					},
					"containerOptions": {
						"async": true,
						"height": "100%"
					}
				}
			}
		}
	}
}
