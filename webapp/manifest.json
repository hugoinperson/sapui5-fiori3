{
	"_version": "1.12.0",
	"sap.app": {
		"id": "utg.ui5.fiori3",
		"type": "application"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"rootView": {
			"viewName": "utg.ui5.fiori3.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "utg.ui5.fiori3.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/todoitems.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"async": true,
				"viewType": "XML",
				"viewPath": "utg.ui5.fiori3.view",
				"controlId": "indexApp",
				"transition": "show",
				"controlAggregation": "pages"
			},
			"routes": {
				"welcome": {
					"pattern": "",
					"target": "welcome"
				}
			},
			"targets": {
				"welcome": {
					"viewName": "Main"
				}
			}
		}
	}
}
